
<ScaleWorkItemsLabel@Label>:
	_scale: 1. if self.texture_size[0] < self.width else float(self.width) / self.texture_size[0]
	canvas.before:
		PushMatrix
		Scale:
			origin: self.center
			x: self._scale or 1.
			y: self._scale or 1.
	canvas.after:
		PopMatrix


<BoxButton@ButtonBehavior+BoxLayout>:
	orientation: 'vertical'
	padding: '8dp'
	canvas.before:
		Color:
			rgb: (1, 1, 1,)
		BorderImage:
			pos: self.pos
			size: self.size
			source: 'data/round.png'




<WorkItems>:
    id:WorkItems
    FloatLayout:

        canvas:
            Color:
                rgba: 232 / 255., 232 / 255., 232 / 255., 1
            BorderImage:
                pos: self.pos
                size: self.size
                source: 'atlas://textures/images/traco-atlas/round'

        BoxLayout:
            orientation: "vertical"

            ActionBar:
                pos_hint: {'top':1}
                canvas:
                    Color:
                        rgba: (45 / 255., 117 / 255., 180 / 255., 1.0)
                    BorderImage:
                        pos: self.pos
                        size: self.size
                        source: 'atlas://textures/images/traco-atlas/round'
                ActionView:
                    use_separator: True
                    ActionPrevious:
                        title:('User Name')
                        with_previous: False
                        app_icon:'atlas://textures/images/traco-atlas/round'

                    ActionOverflow:
                    ActionButton:
                        text:
                            ('@ Office Since \n'
                            '      [color=cbcbcb]10 : 23[/color]')
                        markup: True

                    ActionButton:
                        Image:
                            source: 'atlas://textures/images/traco-atlas/log_out'
                            center: self.parent.center

            StackLayout:
                orientation: 'rl-tb'

                ScaleLabel:
                    size_hint_y: 0.05
                    text:u"[size=15sp][color=666666]Workitems of 7 match [/color][/size]"
                    markup:True
                    padding: dp(12), dp(12)
                    font_size: self.height * 0.6

                ScrollView:
                    #size_hint:(None, None)
                    #size:root.width, root.height - 80
                    do_scroll_x:False

                    GridLayout:
                        cols:1
                        size_hint:(None, None)
                        width:root.width
                        height:root.height + 10

                        BoxLayout:
                            orientation: "vertical"
                            padding: '10dp'
                            spacing: '10dp'
                            size_hint_y: .30

                            BoxLayout:
                                orientation: 'vertical'
